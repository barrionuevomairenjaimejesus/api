    <div id="mapid" style="width: 100%; height: 705px;"></div>
    <script>

    	var mymap = L.map('mapid').setView([51.505, -0.09], 4);

    	L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpejY4NXVycTA2emYycXBndHRqcmZ3N3gifQ.rJcFIG214AriISLbB6B5aw', {
    		maxZoom: 18,
    		attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, ' +
    			'<a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, ' +
    			'Imagery © <a href="https://www.mapbox.com/">Mapbox</a>',
    		id: 'mapbox/streets-v11',
    		tileSize: 512,
    		zoomOffset: -1
    	}).addTo(mymap);


    	//definir un círculo
      <% for (var i = 0; i < muertes_prematuras.length; i++ ) { %>
    	L.circle([
        <%= muertes_prematuras[i].Lenght %>,
        <%= muertes_prematuras[i].Magnitude %>],
        <%= muertes_prematuras[i].Total_deaths %>,{

    		color: 'red',
    		fillColor: '#F6F432',
    		fillOpacity: 0.5
    	}).addTo(mymap).bindPopup("Death population per 100k");
      <% } %>
</script>
